{
	"name": "jsonata-extended",
	"version": "0.3.4",
	"description": "Extended JSONata object with custom functions",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/martinholden-skillsoft/jsonata-extended.git"
	},
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint src/**/*.js",
		"mocha": "node ./node_modules/mocha/bin/mocha \"test/**/*.js\"",
		"doc": "rimraf doc/* && jsdoc --readme README.md --configure jsdoc.json .",
		"pretest": "npm run lint",
		"test": "npm run mocha",
		"posttest": "npm run doc && npm run browserify && npm run minify",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && npm run changelog && git push ",
		"changelog": "rimraf CHANGELOG.md && auto-changelog --template keepachangelog && git add . && git commit . -m changelog && git push",
		"coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
		"check-coverage": "istanbul check-coverage -statement 100 -branch 100 -function 100 -line 100",
		"browserify": "browserify src/index.js --outfile jsonata-extended.js --standalone jsonata-extended",
		"mkdir-dist": "mkdirp ./dist",
		"regenerator": "babel src --out-dir dist --presets=@babel/env",
		"minify": "uglifyjs jsonata-extended.js -o jsonata-extended.min.js --compress --mangle"
	},
	"keywords": [
		"JSONata"
	],
	"author": "Martin Holden &lt;martin.holden@skillsoft.com&gt;",
	"license": "MIT",
	"dependencies": {
		"countries-list": "^2.5.6",
		"fold-to-ascii": "^5.0.0",
		"html-to-text": "^5.1.1",
		"jsonata": "^1.8.3",
		"lodash": "^4.17.19",
		"moment": "^2.27.0",
		"mustache": "^4.0.1",
		"rfc5646": "^3.0.0",
		"url-parse": "^1.4.7",
		"uuid-encoder": "^1.2.0",
		"uuid-validate": "0.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"auto-changelog": "^2.2.0",
		"babel-eslint": "^10.1.0",
		"browserify": "^16.5.1",
		"browserify-shim": "^3.8.14",
		"chai": "^4.2.0",
		"coveralls": "^3.1.0",
		"docdash": "^1.2.0",
		"eslint": "^7.6.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jsdoc": "^3.6.5",
		"mocha": "^8.1.0",
		"nyc": "^15.1.0",
		"prettier": "^2.0.5",
		"regenerator": "^0.14.7",
		"request": "^2.88.0",
		"rimraf": "^3.0.2",
		"uglify-es": "^3.3.9"
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browser": {
		"jsonata": "./node_modules/jsonata/jsonata.js"
	},
	"browserify-shim": {
		"jsonata": "global:jsonata"
	}
}
